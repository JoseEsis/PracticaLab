program ejercicio5;

var
  n: integer;

begin

  write('Ingrese un numero del 1 al 7: ');
  readln(n);

  if n = 1 then writeln('Lunes') else
  if n = 2 then writeln('Martes') else
  if n = 3 then writeln('Miercoles') else
  if n = 4 then writeln('Jueves') else
  if n = 5 then writeln('Viernes') else
  if n = 6 then writeln('Sabado') else
  if n = 7 then writeln('Domingo') else
  writeln('Numero incorrecto');
  readln;
end.
